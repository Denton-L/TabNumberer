{
	"manifest_version": 2,
	"name": "Tab Numberer",
	"version": "0.1.1",
	"description": "A Chrome extension to number your tabs.",
	"browser_action": {
		"default_icon": {
			"16": "icons/icon16.png",
			"24": "icons/icon24.png",
			"32": "icons/icon32.png"
		},
		"default_title": "Tab Numberer"
	},
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"background": {
		"scripts": ["scripts/background.js"]
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["scripts/content.js"]
		}
	]
}
